begin
  cadd(cxx,z,w);
end;


{---------------------------------------------------------------------------}
procedure csubxx(const z: complex; var w: complex);
const
  cxx: complex = (re: 2.0; im: -0.75);
begin
  csub(z,cxx,w);
end;


{---------------------------------------------------------------------------}
procedure cmulxx(const z: complex; var w: complex);
const
  cxx: complex = (re: 2.0; im: -0.75);
begin
