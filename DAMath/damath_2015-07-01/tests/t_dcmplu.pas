end;


{---------------------------------------------------------------------------}
procedure cdivxx(const z: complex; var w: complex);
const
  cxx: complex = (re: 2.0; im: -0.75);
begin
  cdiv(cxx,z,w);
end;


{---------------------------------------------------------------------------}
procedure cagm2(const z: complex; var w: complex);
begin
  cexp(z,w);
  cagm(z,w,w);
end;


{---------------------------------------------------------------------------}
procedure gotest1(const fname: FNamStr; f1: cfunc1; const fv: array of complex);
var
  i: integer;
  w,d: complex;
  af,ad,r,ml: double;
begin
  {$ifdef fullwrite}
    writeln('Test for function ', fname);
  {$else}
    write(fname:10);
  {$endif}
  ml := 0.0;
  for i:=low(zv) to high(zv) do begin
    f1(zv[i], w);
    csub(w, fv[i], d);
    ad := cabs(d);
    af := cabs(fv[i]);
    r := ad;
    {$ifdef fullwrite}
      if af>0.5 then begin
        r := r/af;
        writeln(i:4, '  ',r:20, ' r', '  ',r/eps_d:6:2);
      end
      else writeln(i:4, '  ',r:20, ' a', '  ',r/eps_d:6:2);
    {$else}
      if af>0.5 then r := r/af;
    {$endif}
    ml := maxd(ml,r);
  end;
  {$ifndef fullwrite}
     writeln(' ',ml/eps_d:6:2);
  {$endif}
  maxr := maxd(ml,maxr);
end;


{---------------------------------------------------------------------------}
procedure gotestx(const fname: FNamStr; fx: cfuncx; const fv: array of double);
var
  i: integer;
  af,ad,r,ml: double;
begin
  {$ifdef fullwrite}
    writeln('Test for function ', fname);
  {$else}
