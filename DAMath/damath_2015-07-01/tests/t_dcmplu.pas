    {$else}
      if af>0.5 then r := r/af;
    {$endif}
    ml := maxd(ml,r);
  end;
  {$ifndef fullwrite}
     writeln(' ',ml/eps_d:6:2);
  {$endif}
  maxr := maxd(ml,maxr);
end;


{---------------------------------------------------------------------------}
procedure testpoly(const fname: FNamStr; ronly: boolean; const fv: array of complex);
var
  i: integer;
  w,d,z: complex;
  af,ad,r,ml: double;
  ra: array[0..20] of double;
begin
  {$ifdef fullwrite}
    writeln('Test for function ', fname);
  {$else}
    write(fname:10);
  {$endif}
  ml := 0.0;
  z.re := 0.5;
  z.im := -1.25;
  for i:=low(zv) to high(zv) do begin
    if ronly then begin
      if i<20 then ra[i] := zv[i].im;
      cpolyr(z, ra, i+1, w);
    end
    else cpoly(z, zv, i+1, w);
    csub(w, fv[i], d);
    ad := cabs(d);
    af := cabs(fv[i]);
    r := ad;
    {$ifdef fullwrite}
      if af>0.5 then begin
        r := r/af;
        writeln(i:4, '  ',r:20, ' r', '  ',r/eps_d:6:2);
      end
      else writeln(i:4, '  ',r:20, ' a', '  ',r/eps_d:6:2);
    {$else}
      if af>0.5 then r := r/af;
    {$endif}
    ml := maxd(ml,r);
  end;
  {$ifndef fullwrite}
     writeln(' ',ml/eps_d:6:2);
  {$endif}
  maxr := maxd(ml,maxr);
end;


