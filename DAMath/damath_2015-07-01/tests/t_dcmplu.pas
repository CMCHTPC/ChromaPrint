  u.re := 0.8600470153764810145;
  u.im := -6.283185307109270813;
  test1(9);

  z.re := -1.5;
  z.im := -1e-10;
  clngamma(z,w);
  u.re := 0.8600470153764810145;
  u.im := 6.283185307109270813;
  test1(10);

  z.re := 0.75;
