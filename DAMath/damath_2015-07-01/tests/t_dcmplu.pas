  CNEF = 6;

begin
  maxr := 0;
  writeln('Test DAMCmplx V', DAMCmplx_Version, ' - DAMath based Complex routines    (c) W.Ehrhardt 2013-2014');

  gotestx('cabs',      {$ifdef FPC_ProcVar}@{$endif}cabs,      t_abs);
