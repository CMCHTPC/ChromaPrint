  procedure singletest(n: integer; const a: complex; x,y: double);
    {-Return abs((a-b)/b), b=x+iy, special if b=0, or a-b=0}
  var
    b,c: complex;
    r: double;
  begin
    inc(st_cnt);
    cset(b,x,y);
    csub(a,b,c);
    if (c.re=0.0) and (c.im=0.0) then r := 0.0
    else begin
      if (b.re<>0.0) or (b.im<>0.0) then cdiv(c,b,c);
      r := cabs(c);
    end;
    if r > eps then begin
      writeln('  Test ',n, ' failed: rel. error = ', r:20);
      inc(st_fail);
    end;
