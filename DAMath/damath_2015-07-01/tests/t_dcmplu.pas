{---------------------------------------------------------------------------}
procedure test2(k,n: integer);
  {-test |u-w|/|u| against n*eps_d}
var
  v: complex;
  eps: double;
begin
  inc(numtest);
  eps := n*eps_d;
  if u.re=0 then v.re := w.re-u.re else v.re := 1.0-w.re/u.re;
  if u.im=0 then v.im := w.im-u.im else v.im := 1.0-w.im/u.im;
  if (abs(v.re) > eps) or (abs(v.im) > eps) then begin
    writeln(k:6,v.re:30, v.im:30, maxd(abs(v.re), abs(v.im))/eps_d:8:2);
    inc(fail);
  end;
end;


{---------------------------------------------------------------------------}
procedure test2a(k,n: integer);
  {-test |u-w| against n*eps_s}
var
  v: complex;
  eps: double;
begin
  inc(numtest);
  eps := n*eps_d;
  v.re := w.re-u.re;
  v.im := w.im-u.im;
  if (abs(v.re) > eps) or (abs(v.im) > eps) then begin
    writeln(k:6,v.re:29, v.im:29, maxd(abs(v.re), abs(v.im))/eps_d:14:2);
    inc(fail);
  end;
end;



